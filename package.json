    "tape": "^4.15.1",
    "nyc": "^15.1.0"
  },
  "nyc": {
    "check-coverage": true,
    "branches": 85,
    "functions": 50,
    "statements": 25,
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "report-dir": "build/coverage",
